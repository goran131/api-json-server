{
    "functions": {
        "api/server.js": {
            "memory": 1024,
            "includeFiles": "skiv-data.json"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "api/server.js"
        }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "https://labb-3-skivregister.vercel.app" }
        ]
      },
      {
        "source": "/labb-3-skivregister.vercel.app/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "https://labb-3-skivregister.vercel.app" }
        ]
      }
  ]

}
